.type Instance <: number
.type Subnet <: symbol
.type Tag <: symbol

.decl hasSubnet(i: Instance, n: Subnet)
.decl hasTag(i: Instance, t: Tag)
.decl subnet(n: Subnet)
.decl tag(t: Tag)

.decl q(i: Instance)

// query instances in subnet "web" with tag "bastion"
.input hasSubnet
.input hasTag
.input subnet
.input tag

q(i) :- hasSubnet(i, w), hasTag(i, t), subnet(w), tag(t).
.output q

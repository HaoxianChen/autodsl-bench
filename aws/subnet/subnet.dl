.type Instance <: symbol
.type Subnet <: symbol
.type Tag <: symbol

.decl hasSubnet(i: Instance, n: Subnet)
.decl hasTag(i: Instance, t: Tag)
.decl q(i: Instance)

// query instances in subnet "web" with tag "bastion"
.input hasSubnet
.input hasTag

q(i) :- hasSubnet(i, "web"), hasTag(i, "bastion").
.output q
